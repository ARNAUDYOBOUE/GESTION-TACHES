datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")  // Utilise la variable d'environnement DATABASE_URL
}

generator client {
  provider = "prisma-client-js"
}


model otherUser {
  id        Int    @id @default(autoincrement())
  email     String @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
}

model OtherUser {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

